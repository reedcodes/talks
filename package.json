{
  "name": "11ty-reveal",
  "version": "0.1.0",
  "description": "Eleventy-powered reveal.js slideshow generator",
  "author": "Reed Piernock",
  "license": "GPL-3.0-only",
  "homepage": "https://reedcodes.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/reedcodes/11ty-reveal"
  },
  "bugs": {
    "url": "https://github.com/reedcodes/11ty-reveal/issues"
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.*",
    "@11ty/eleventy-img": "2.0.*",
    "@11ty/eleventy-navigation": "0.3.*",
    "a11y-color-tokens": "0.6.*",
    "eleventy-plugin-gen-favicons": "1.1.*",
    "eleventy-plugin-metagen": "1.7.*",
    "gulp": "4.0.*",
    "gulp-clean-css": "4.3.*",
    "gulp-sass": "5.1.*",
    "gulp-sourcemaps": "3.0.*",
    "include-media": "1.4.*",
    "npm-run-all": "4.1.*",
    "reveal.js": "4.3.*",
    "sass": "1.54.*"
  },
  "main": "gulpfile.js",
  "scripts": {
    "tokens": "npx a11y-color-tokens --colorTokensPath='source/_data/colors.js' --outputDirPath='source/_sass/color' --compatibilityDocsPath='source/guide'",
    "build:gulp": "gulp",
    "build:11ty": "npx eleventy",
    "build:fresh": "npm-run-all tokens build:gulp build:11ty",
    "build": "npm-run-all build:gulp build:11ty",
    "clean:up": "rm -rf ./site/",
    "clean": "npm-run-all clean:up tokens build:gulp build:11ty"
  }
}
